{
  "name": "@hcikit/workflow",
  "version": "0.2.0",
  "description": "A workflow manager for running experiments using React.",
  "author": "blainelewis1",
  "license": "MIT",
  "repository": "hcikit/hcikit",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --colors",
    "test:coverage": "jest --coverage --colors",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/registerServiceWorker.js",
      "!src/stories/*",
      "!src/experiments/*",
      "!src/setupTests.js",
      "!src/**/*.stories.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es).+\\.js$"
    ]
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-redux": "^7.1.0"
  },
  "dependencies": {
    "lodash-es": "^4.17.11",
    "prop-types": "^15.7.2",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "react": "^16.8.6",
    "react-redux": "^7.0.3"
  },
  "gitHead": "8d0f2af41121f70996e8b5803d006832b147fd86"
}
