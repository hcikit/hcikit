{
  "name": "@hcikit/workflow",
  "version": "1.0.0-alpha.2",
  "description": "A workflow manager for running experiments using React.",
  "author": "blainelewis1",
  "license": "MIT",
  "repository": "hcikit/hcikit",
  "main": "./dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "start": "tsc",
    "build": "tsc --build && cp \"./package.json\" ./dist/",
    "test": "jest",
    "test:watch": "jest --watch --colors --verbose",
    "test:coverage": "jest --coverage --colors",
    "prepack": "yarn run build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es).+\\.js$"
    ]
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "lodash": "^4.17.15",
    "regenerator-runtime": "^0.13.5"
  },
  "devDependencies": {
    "@types/deep-freeze": "^0.1.2",
    "@types/jest": "^26.0.24",
    "@types/lodash": "^4.14.171",
    "deep-freeze": "^0.0.1",
    "jest": "^27.0.6",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "gitHead": "8d0f2af41121f70996e8b5803d006832b147fd86"
}