{
  "name": "@hcikit/workflow",
  "version": "0.1.2",
  "description": "A workflow manager for running experiments using React.",
  "author": "blainelewis1",
  "license": "MIT",
  "repository": "hcikit/hcikit",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch --colors",
    "test:coverage": "jest --coverage --colors",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/registerServiceWorker.js",
      "!src/stories/*",
      "!src/experiments/*",
      "!src/setupTests.js",
      "!src/**/*.stories.js"
    ]
  },
  "peerDependencies": {
    "react": "^16.8.6",
    "react-redux": "^7.0.3",
    "styled-components": "^4.2.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash-es": "^4.17.11",
    "marked": "^0.6.2",
    "prop-types": "^15.7.2",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.3"
  },
  "gitHead": "5cfb14b397c894b08e9afff3262ef9377ccdacc7"
}
