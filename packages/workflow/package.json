{
  "name": "@hcikit/workflow",
  "version": "1.0.0-alpha.8",
  "description": "A workflow manager for running experiments.",
  "author": "blainelewis1",
  "license": "MIT",
  "repository": "hcikit/hcikit",
  "module": "dist/index.js",
  "main": "dist/index.js",
  "type": "module",
  "keywords": [
    "hci",
    "experiment"
  ],
  "types": "./dist/index.d.ts",
  "exports": {
    "import": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "start": "tsc",
    "build": "yarn run generate-schema && tsc --build --verbose",
    "test": "jest",
    "test:watch": "jest --watch --colors --verbose",
    "test:coverage": "jest --coverage --colors",
    "prepack": "yarn run build",
    "generate-schema": "ts-json-schema-generator --tsconfig tsconfig.json -p src/types.ts -t ConfigurationNoGenerics > src/schemas.json"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/registerServiceWorker.js",
      "!src/setupTests.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es).+\\.js$"
    ]
  },
  "dependencies": {
    "core-js": "^3.17.3",
    "lodash": "^4.17.21",
    "regenerator-runtime": "^0.13.9"
  },
  "devDependencies": {
    "@types/deep-freeze": "^0.1.2",
    "@types/jest": "^27.0.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.9.1",
    "deep-freeze": "^0.0.1",
    "jest": "^27.2.0",
    "ts-jest": "^27.0.5",
    "ts-json-schema-generator": "^0.95.0",
    "typescript": "^4.4.3"
  },
  "gitHead": "8d0f2af41121f70996e8b5803d006832b147fd86"
}
