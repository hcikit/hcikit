{
  "name": "@hcikit/tasks",
  "version": "0.2.0",
  "description": "A set of components that act as experimental tasks for running HCI experiments",
  "author": "blainelewis1",
  "license": "MIT",
  "repository": "hcikit/hcikit",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/registerServiceWorker.js",
      "!src/stories/*",
      "!src/experiments/*",
      "!src/setupTests.js",
      "!src/**/*.stories.js"
    ],
    "transformIgnorePatterns": [
      "/node_modules/(?!lodash-es).+\\.js$"
    ],
    "setupFilesAfterEnv": [
      "./setupTests.js"
    ]
  },
  "peerDependencies": {
    "@hcikit/workflow": "0.x",
    "@material-ui/core": "^4.1.1",
    "react": "^16.8.6",
    "react-redux": "^7.1.0",
    "styled-components": "^4.3.1"
  },
  "dependencies": {
    "lodash-es": "^4.17.11",
    "markdown-to-jsx": "^6.10.2",
    "prop-types": "^15.7.2"
  },
  "gitHead": "8d0f2af41121f70996e8b5803d006832b147fd86",
  "devDependencies": {
    "react-scripts": "^3.0.1",
    "react-styleguidist": "^9.1.8"
  }
}
