{
  "name": "@hcikit/tasks",
  "version": "0.1.0",
  "description": "A set of components that act as experimental tasks for running HCI experiments",
  "author": "blainelewis1",
  "license": "MIT",
  "repository": "hcikit/hcikit",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "jest --env=jsdom",
    "test:watch": "jest --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!/node_modules/",
      "!src/registerServiceWorker.js",
      "!src/stories/*",
      "!src/experiments/*",
      "!src/setupTests.js",
      "!src/**/*.stories.js"
    ]
  },
  "peerDependencies": {
    "@material-ui/core": "^3.9.3",
    "react": "^16.8.6",
    "react-redux": "^7.0.3",
    "styled-components": "^4.2.0",
    "@hcikit/workflow": "^0.1.0"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "lodash": "^4.17.11",
    "marked": "^0.6.2",
    "prop-types": "^15.7.2"
  }
}